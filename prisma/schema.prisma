generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model products {
  id             String  @id @default(auto()) @map("_id") @db.ObjectId
  category       String
  categoryUrl    String
  currency       String?
  name           String
  numberOfOffers Int
  position       Int
  price          Float
  thumbnail      String
  url            String
}

model test {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
