generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model products {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  product_id     String   @unique
  refreshed_at   DateTime
  position       Int
  category       String
  name           String
  url            String
  rating         Float?
  reviewCount    Int?
  thumbnail      String
  price          Float
  currency       String?
  numberOfOffers Int
}

model test {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
